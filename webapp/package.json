    "@types/react": "18.2.47",
    "@types/redux-logger": "3.0.12",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "cross-env": "7.0.3",
    "css-loader": "6.9.0",
    "eslint": "8.56.0",
    "eslint-plugin-react": "7.33.2",
    "favicons": "7.1.5",
